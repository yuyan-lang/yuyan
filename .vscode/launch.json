{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "python py-interpreter/c_compiler.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/py-interpreter/c_compiler.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "豫言编译器/入口"
            ]
        },
        {
            "name": "python py-interpreter/interpreter.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/py-interpreter/interpreter.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "args": [
                "豫言编译器/入口"
            ]
        },
        {
            "name": "./yy_exec_debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/yy_exec_debug",
            "args": [
                "5",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [
                {
                    "name": "YY_GC_DEBUG_FLAG",
                    "value": "1"
                },
                {
                    "name": "YY_DEBUG_FLAG",
                    "value": "1"
                },
            ],
            "externalConsole": false,
            "setupCommands": [
                // {
                //     "text": "handle SIGSEGV noprint nostop pass",
                //     "description": "ingore libgc related errors",
                //     "ignoreFailures": false
                // },
                {
                    "text": "set output-radix 16"
                },
                {
                    "text": "set disable-randomization off"
                }
            ],
            // "MIMode": "lldb",
            // "miDebuggerPath": "/usr/bin/lldb"
        },
        {
            "name": "./yy_exec_debug yylib/标准库。豫",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/yy_exec_debug",
            "args": [
                "yylib/标准库。豫",
                "--debug-print-add",
                "log_print_tree"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [
                {
                    "name": "YY_GC_DEBUG_FLAG",
                    "value": "1"
                },
                {
                    "name": "YY_DEBUG_FLAG",
                    "value": "1"
                },
            ],
            "externalConsole": false,
            "setupCommands": [
                // {
                //     "text": "handle SIGSEGV noprint nostop pass",
                //     "description": "ingore libgc related errors",
                //     "ignoreFailures": false
                // }
                {
                    "text": "set output-radix 16"
                },
            ],
            // "MIMode": "lldb",
            // "miDebuggerPath": "/usr/bin/lldb"
        },
        {
            "name": "NodeJS Debug bs",
            // "program": "${file}",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "args": [
                // "${input:filepath}"
                "--stack-size=8192",
                "./yy_bs_bs.js",
                "yylib/runtest.yuyan",
                "--do-not-load-cache",
                // "-vvv",
                // "--parse-only"
                // "yy_bs_bs.js", 
                // "yylib/runtest.yuyan"
            ]
        },
        {
            "name": "NodeJS Debug Current File",
            "program": "${file}",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
        },
        {
            "name": "parallel_compile.py",
            "type": "python",
            "request": "launch",
            "program": "parallel_compile.py",
            "console": "integratedTerminal",
            "args": [
                "performance-investigation/matrix-multiply/matrix-multiply.yuyan",
                "--extra=--very-parallel"
            ],
            "justMyCode": true
        }
    ],
    "inputs": [
        {
            "id": "filepath",
            "type": "promptString",
            "description": "Input file"
        }
    ]
}